{
    "name": "alive2212/laravel-mobile-passport",
    "description": "This is an illuminate package for one time authentication (OTP) with OAuth2 Base",
    "license": "MIT",
    "authors": [
        {
            "name": "Babak Nodoust",
            "email": "alive2212@yahoo.com",
            "homepage": "http://babakn.com"
        }
    ],
    "homepage": "https://github.com/alive2212/laravelmobilepassport",
    "keywords": ["Laravel", "LaravelMobilePassport"],
    "require": {
        "peterpetrus/passport-token": "^0.0",
        "illuminate/support": "~5",
        "alive2212/laravel-smart-restful": "^2",
        "laravel/passport": "^8"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Alive2212\\LaravelMobilePassport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alive2212\\LaravelMobilePassport\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alive2212\\LaravelMobilePassport\\LaravelMobilePassportServiceProvider"
            ],
            "aliases": {
                "LaravelMobilePassport": "Alive2212\\LaravelMobilePassport\\Facades\\LaravelMobilePassport"
            }
        }
    }
}